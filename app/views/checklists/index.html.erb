<h1>Checklists
<%= link_to '+', new_checklist_path %>
</h1>
<table>
	<% @checklists.each do |checklist| %>
		<tr>
			<td><%= checklist.name %></td>
			<td><%= link_to 'View', checklist %></td>
			<td><%= link_to 'Edit', edit_checklist_path(checklist) %></td>
			<td><%= link_to 'Delete', checklist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
		</tr>
	<% end %>
</table>

<br />

